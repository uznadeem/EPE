@model FYP.ViewModel.FormResultViewModel
@{
    ViewBag.Title = "FormResult";
}

<h2>Form Data</h2>

<div class="container">

    @if (Model.Ques.Count == 0)
    {
        <p>Form is Empty Click Here to add Question @Html.ActionLink("AddQuestion", "AddQuestion", new { controller = "Admin", c_id = Model.comid, qf_id = Model.qf_id }) </p>

    }
    else
    {

        for (var i = 0; i < Model.Ques.Count; i++)
        {
            <p>@Model.Ques[i].QuestionString  @Html.ActionLink("Result", "Graphview", new { qid = Model.Ques[i].QuestionID })</p>

        }

    }

</div>